{
  "text": {
    "normal": {
      "color": "white",
      "fontSize": "22px",
      "textAlign": "left",
      "shadow": true
    },
    "small": {
      "color": "black",
      "fontSize": "16px",
      "textAlign": "left",
      "shadow": true
    },
    "large": {
      "color": "black",
      "fontSize": "30px",
      "textAlign": "left",
      "shadow": true
    },
    "title": {
      "color": "firebrick",
      "fontSize": "50px",
      "textAlign": "left",
      "shadow": true
    },
    "subtitle": {
      "color": "black",
      "fontSize": "30px",
      "textAlign": "center",
      "shadow": true
    }
  },
  "button":{
    "color":"firebrick",
    "textColor":"white",
    "hoverColor":"red",
    "fontSize":"15px",
    "margin": "0px 0px 0px 0px",
    "border":{
      "style":"solid",
      "color":"#cc000000",
      "width":"1px 1px 1px 1px",
      "radius":"30px"
    }
  },
    "accordion":{
      "BGColor":"#fafafa",
      "textColor":"black",
      "title":{
        "color":"black"
      },
      "item":{
        "BGColor":"white"
      },
      "border":{
        "style":"none",
        "color":"#cc0000ff",
        "width":"1px 1px 1px 1px",
        "radius":"8px"
      },
      "transition":{
        "duration":"0.3s",
        "cubicBezier":"0.42, 0, 0.58, 1.0"
      }
    },
    "header":{
      "textColor":"white",
      "BGPosition":"center center",
      "link":{
        "color":"white",
        "textAlgin":"center"
      },
      "navBar":{
        "margin":"0px 160px 0px 160px"
      },
      "feature":{
        "inlineSize":"500px",
        "align":"flex-start",
        "padding":"100px 0px 500px 0px",
        "margin":"0px 160px 0px 160px"
      },
      "text":{
        "color":"",
        "textAlgin":"left",
        "shadow":true
      },
      "featureCallOut":{
        "color":"firebrick",
        "textAlgin":"left",
        "shadow":true
      },
      "button":{
        "color":"firebrick",
        "textColor":"white",
        "hoverColor":"red",
        "fontSize":"15px",
        "border":{
          "style":"solid",
          "color":"#cc000000",
          "width":"1px 1px 1px 1px",
          "radius":"30px"
        }
      }
    },
    "jumbotron":{
      "pane":{
        "margin":"0px 0px 0px 0px",
        "padding":"0px 80px 0px 0px"
      },
      "item":{
        "border":{
          "style":"none",
          "color":"black",
          "width":"1px 1px 1px 1px"
        },
        "title":{
          "color":"black",
          "size":"40px",
          "fontWeight":"700",
          "align":"left",
          "inlineSize":"400px"
        },
        "subTitle":{
          "color":"black",
          "size":"19px",
          "fontWeight":"400",
          "align":"left",
          "inlineSize":"250px"
        }
      }
    },
    "grid":{
      "BGColor":"darkgrey",
      "containerPadding":"90px 80px",
      "columnPadding":"0 50px",
      "columnSize":"400px"
    }
  }